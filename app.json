{
  "name": "silva-md",
  "description": "Javascript WhatsApp bot made by Silva.",
  "keywords": ["silva-md", "whatsapp", "baileys", "bot"],
  "success_url": "/",
  "stack": "heroku-24",
  "env": {
    "SESSION_ID": {
      "description": "Put the session-id here.",
      "required": true,
      "value": ""
    },
    "ALIVE_IMG": {
      "description": "Paste your image URL here.",
      "required": false,
      "value": "https://files.catbox.moe/5uli5p.jpeg"
    },
    "LIVE_MSG": {
      "description": "Paste your alive message here.",
      "required": false,
      "value": "> SILVA MD IS ACTIVE AND RUNNING ⚡"
    },
    "ANTIDELETE_PRIVATE": {
      "description": "Restore deleted messages in private chat.",
      "required": true,
      "value": "true"
    },
    "ANTIDELETE_GROUP": {
      "description": "Restore deleted messages in groups.",
      "required": true,
      "value": "true"
    },
    "PREFIX": {
      "description": "Bot command prefix.",
      "required": false,
      "value": "."
    },
    "MODE": {
      "description": "Bot mode: public, private, group, inbox.",
      "required": false,
      "value": "public"
    },
    "AUTO_STATUS_REACT": {
      "description": "Enable auto status reactions.",
      "required": false,
      "value": "true"
    },
    "ALWAYS_ONLINE": {
      "description": "Keep bot always online.",
      "required": false,
      "value": "false"
    },
    "AUTO_STATUS_SEEN": {
      "description": "Enable auto status seen.",
      "required": true,
      "value": "true"
    },
    "AUTO_STATUS_REPLY": {
      "description": "Enable auto status reply.",
      "required": true,
      "value": "false"
    },
    "AUTO_STATUS__MSG": {
      "description": "Message for auto status reply.",
      "required": true,
      "value": "*👀 Seen by Silva MD ✅*"
    },
    "OWNER_NAME": {
      "description": "Bot owner name.",
      "required": false,
      "value": "Silva MD"
    },
    "OWNER_NUMBER": {
      "description": "Owner WhatsApp number.",
      "required": false,
      "value": "254700143167"
    },
    "BOT_NAME": {
      "description": "Bot display name.",
      "required": false,
      "value": "✦ Silva ✦ MD ✦"
    },
    "ANTI_LINK": {
      "description": "Enable anti-link feature in groups.",
      "required": true,
      "value": "true"
    },
    "ANTI_BAD": {
      "description": "Enable anti-bad words feature.",
      "required": false,
      "value": "false"
    },
    "DESCRIPTION": {
      "description": "Caption for menu and bot branding.",
      "required": false,
      "value": "*© ✦ Silva ✦ MD ✦*"
    },
    "DELETE_LINKS": {
      "description": "Remove links from group without removing member.",
      "required": false,
      "value": "true"
    },
    "AUTO_RECORDING": {
      "description": "Enable auto recording simulation.",
      "required": false,
      "value": "false"
    },
    "AUTO_TYPING": {
      "description": "Enable auto typing simulation.",
      "required": false,
      "value": "true"
    },
    "AUTO_REACT": {
      "description": "Enable auto reactions for all messages.",
      "required": false,
      "value": "false"
    },
    "OWNER_REACT": {
      "description": "React only to owner messages.",
      "required": false,
      "value": "false"
    },
    "CUSTOM_REACT": {
      "description": "Enable custom reactions.",
      "required": false,
      "value": "false"
    },
    "CUSTOM_REACT_EMOJIS": {
      "description": "Custom emojis for reactions.",
      "required": false,
      "value": "💝,💖,💗,❤️‍🔥,❤️‍🩹,❤️,🩷,🧡,💛,💚,💙,🩵,💜,🤎,🖤,🩶,🤍"
    },
    "READ_MESSAGE": {
      "description": "Enable automatic message read receipts.",
      "required": false,
      "value": "false"
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
    }
  ]
}
